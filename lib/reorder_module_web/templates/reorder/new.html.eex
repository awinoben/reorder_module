<h2>New Reorder</h2>


<%= form_for @reorder_changeset, product_reorder_path(@conn, :create, @product), fn f -> %>
  <%= label f, :amount, class: "control-label" %>
  <%= text_input f, :amount, class: "form-control" %>
  <%= error_tag f, :amount %>
  <%= submit "Submit", class: "btn btn-primary" %>
<% end %>
